<%= form_for @profile, url: user_profile_path do |f| %>
    <%= f.label :city %>
    <%= f.text_field :city, :size=>"40" %>
    <%= f.label :fav_quote %>
    <%= f.text_field :fav_quote, :size=>"40" %>
    <%= f.label :goal %>
    <%= f.text_field :goal, :size=>"40" %>
    <%= f.submit "Create Profile" %>
<% end %>